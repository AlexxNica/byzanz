msgstr "Pixel"

#: ../src/record.c:41
msgid "Y coordinate of rectangle to record"
msgstr "Coordenada Y del rectángulo a grabar"

#: ../src/record.c:42
msgid "Width of recording rectangle"
msgstr "Ancho del rectángulo a grabar"

#: ../src/record.c:43
